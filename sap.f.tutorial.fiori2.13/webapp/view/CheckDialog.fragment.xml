<core:FragmentDefinition
   xmlns:m="sap.m"
   xmlns:core="sap.ui.core" >
   <m:Dialog
      id="checkDialog"
      title="{i18n>BatchCreation}">
      <m:subHeader>
        <m:OverflowToolbar>
			<m:OverflowToolbarButton
                type="Transparent"
                icon="sap-icon://simulate"
                press=".check" />
            <m:OverflowToolbarButton
                type="Transparent"
                icon="sap-icon://paste"
                press=".firePaste" />
		</m:OverflowToolbar>
      </m:subHeader>
        <m:content>
            <m:Table
                id="checkTable"
                paste=".onPaste"
                items="{path : 'check>/'}">

                <m:columns>
                    <m:Column>
                        <m:Text text="{i18n>ProvinceID}"/>
                    </m:Column>
                    <m:Column>
                        <m:Text text="{i18n>ProvinceDesc}"/>
                    </m:Column>
                    <m:Column>
                        <m:Text text="{i18n>ProtocolID}"/>
                    </m:Column>
                    <m:Column>
                        <m:Text text="{i18n>CustomerID}"/>
                    </m:Column>
                    <!-- <m:Column>
                        <m:Text text="{i18n>CustomerDesc}"/>
                    </m:Column> -->
                    <m:Column>
                        <m:Text text="{i18n>Rate}"/>
                    </m:Column>
                    <m:Column>
                        <m:Text text="{i18n>RateUnit}"/>
                    </m:Column>
                    <m:Column>
                        <m:Text text="{i18n>CAP}"/>
                    </m:Column>
                    <m:Column>
                        <m:Text text="{i18n>ValidFrom}"/>
                    </m:Column>
                    <m:Column>
                        <m:Text text="{i18n>ValidTo}"/>
                    </m:Column>
                    <m:Column>
                    </m:Column>
                </m:columns>
                <m:items>
                    <m:ColumnListItem>
                        <m:cells>
                            <m:Input value="{check>Province}"/>
                        </m:cells>
                        <m:cells>
                            <m:Input value="{check>RegionDesc}"/>
                            <!-- <m:Input value="{new>RateUnit}"/> -->
                        </m:cells>
                        <m:cells>
                            <m:Input value="{check>AgreeId}"/>
                        </m:cells>
                        <m:cells>
                            <m:Input value="{check>BpCodeInAgree}"/>
                        </m:cells>

                        <m:cells>
                            <m:Input value="{check>Rate}"/>
                        </m:cells>
                        <m:cells>
                            <m:Input value="{check>RateUnit}"/>
                        </m:cells>
                        <m:cells>
                            <m:Input value="{check>Cap}"/>
                        </m:cells>
                        <m:cells>
                            <m:DatePicker value="{
								path: 'check>ValidFrom',
                                type: 'sap.ui.model.type.Date',
								formatOptions: {
									style: 'medium',
									pattern: 'yyyy-MM-dd'
								}
							}" valueFormat="yyyy-MM-dd"/>
                        </m:cells>
                        <m:cells>
                            <m:DatePicker value="{
								path: 'check>ValidTo',
                                type: 'sap.ui.model.type.Date',
								formatOptions: {
									style: 'medium',
									pattern: 'yyyy-MM-dd'
								}
							}" valueFormat="yyyy-MM-dd"/>
                        </m:cells>
                        <m:cells>
                            <m:Text text="{check>MessageText}"/>
                        </m:cells>
                    </m:ColumnListItem>
                </m:items>
            </m:Table>
         </m:content>
        <m:beginButton>
            <m:Button
                enabled="{save>/enabled}"
                text="{i18n>Save}"
                press=".onConfirmDialog"/>
        </m:beginButton>
        <m:endButton>
            <m:Button
                text="{i18n>Cancel}"
                press=".onCloseDialog"/>
        </m:endButton>
   </m:Dialog>
</core:FragmentDefinition>